<%= form_for @user do |f| %>
  <%=f.label "email" %>
  <%= f.text_field :email, placeholder: "example@example.com"%><br>
  
  <%=f.label "username"%>
  <%= f.text_field :username, placeholder: "You're username here" %><br>

  <%= f.label "password"%>
  <%= f.password_field :password %><br>

  <%= f.submit %>
  <%end%>







